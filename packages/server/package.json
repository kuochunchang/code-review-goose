{
  "name": "@code-review-goose/server",
  "version": "1.0.0",
  "description": "Goose Code Review Tool - Server",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:standalone": "tsx src/standalone.ts",
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "cors": "^2.8.5",
    "detect-port": "^2.1.0",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "ignore": "^5.3.0",
    "openai": "^6.8.1"
  },
  "devDependencies": {
    "@types/babel__parser": "^7.0.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/cors": "^2.8.17",
    "@types/detect-port": "^1.3.5",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^2.0.0",
    "supertest": "^7.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "server",
    "express",
    "code-review"
  ],
  "author": "",
  "license": "MIT"
}
