    "vue-router": "^4.2.5",
    "vuetify": "^3.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/dompurify": "^3.0.5",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vitest/coverage-v8": "^2.0.0",
    "@vue/test-utils": "^2.4.3",
    "detect-port": "^2.1.0",
