{
  "name": "@goose-sight/web",
  "version": "1.0.1",
  "description": "Goose Code Review Tool - Web UI",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "clean": "rm -rf dist",
